{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "eslint": "eslint './{bin,lib,src,test}/**/*.ts' --max-warnings=0",
    "prettier:check": "prettier --check ..",
    "prettier:write": "prettier --write ..",
    "test": "jest",
    "snap": "jest --updateSnapshot",
    "cdk": "cdk",
    "cdk:diff": "cdk diff || true",
    "cdk:deploy": "cdk deploy --require-approval never"
  },
  "devDependencies": {
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "aws-cdk": "2.76.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.1.0",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.76.0",
    "constructs": "10.2.60",
    "source-map-support": "0.5.21"
  }
}
