{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "eslint": "eslint './{bin,lib,src,test}/**/*.ts' --max-warnings=0",
    "prettier:check": "prettier --check ..",
    "prettier:write": "prettier --write ..",
    "test": "jest",
    "snap": "jest --updateSnapshot",
    "cdk": "cdk",
    "cdk:diff": "cdk diff || true",
    "cdk:deploy": "cdk deploy --require-approval never"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "@types/node": "16.11.58",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "aws-cdk": "2.41.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.41.0",
    "constructs": "10.1.96",
    "source-map-support": "0.5.21"
  }
}
